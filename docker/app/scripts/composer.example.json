{
  "name": "webnitros/modx-extra-docker",
  "minimum-stability": "alpha",
  "authors": [
    {
      "name": "webnitros",
      "email": "info@bustep.ru"
    }
  ],
  "scripts": {
    "tests": "phpunit --testdox --bootstrap bootstrap.php"
  },
  "require": {
    "symfony/dotenv": "^5.2",
    "symfony/var-dumper": "^5.4",
    "symfony/console": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "9.*",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": false,
    "platform": {
      "php": "7.4"
    }
  }
}
